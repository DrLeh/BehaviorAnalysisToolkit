{
  "inputs": [ "SupportFiles/InputFiles/OA5-Breakfast.csv" ],
  "transformers": [ "LinearAccelerationTransformer", "LabelCleanupTransformer" ],
  "filters": [
    {
      "Name": "ActivityFilter",
      "Parameters": [
        {
          "Field": "Label",
          "Clauses": [
            {
              "Key": "Contains",
              "Value": "Select"
            },
            {
              "Key": "Split",
              "Value": "true"
            }
          ]
        }
      ]
    }
  ],
  "analyzers": [
    {
      "Name": "PauseCountAnalysis",
      "Parameters": [
        {
          "Field": "InstantSpeed",
          "Clauses": [
            {
              "Key": "Threshold",
              "Value": "0.01"
            },
            {
              "Key": "Window",
              "Value": "12"
            }
          ]
        }
      ]
    },
    {
      "Name": "PauseDurationAnalysis",
      "Parameters": [
        {
          "Field": "InstantSpeed",
          "Clauses": [
            {
              "Key": "Threshold",
              "Value": "0.01"
            },
            {
              "Key": "Window",
              "Value": "12"
            }
          ]
        }
      ]
    }
  ]
}