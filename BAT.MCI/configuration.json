{
  "inputs": [ "SupportFiles/OA5-Breakfast.csv" ],
  "transformers": [ "LinearAccelerationTransformer", "LabelCleanupTransformer" ],
  "filters": [
    {
      "Name": "ActivityFilter",
      "Predicates": [
        {
          "Field": "Label",
          "Clauses": [
            {
              "Key": "Contains",
              "Value": "Select"
            },
            {
              "Key": "Split",
              "Value": "true"
            }
          ]
        }
      ]
    }
  ]
}