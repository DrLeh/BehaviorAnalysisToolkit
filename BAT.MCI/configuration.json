{
  "inputs": [ "SupportFiles/OA5-Breakfast.csv", "SupportFiles/OA5-Lunch.csv" ],
  "transformers": [ "LinearAcceleration", "LabelCleanup" ],
  "filters": [
    {
      "Name": "Activity",
      "Parameters": [
        {
          "Field": "Label",
          "Clauses": [
            {
              "Key": "Contains",
              "Value": "Select"
            },
            {
              "Key": "Split",
              "Value": "true"
            }
          ]
        }
      ]
    }
  ],
  "analyzers": [
    {
      "Name": "PauseCount",
      "Parameters": [
        {
          "Field": "InstantSpeed",
          "Clauses": [
            {
              "Key": "Threshold",
              "Value": "0.01"
            },
            {
              "Key": "Window",
              "Value": "12"
            }
          ]
        }
      ]
    },
    {
      "Name": "PauseDuration",
      "Parameters": [
        {
          "Field": "InstantSpeed",
          "Clauses": [
            {
              "Key": "Threshold",
              "Value": "0.01"
            },
            {
              "Key": "Window",
              "Value": "12"
            }
          ]
        }
      ]
    },
    { "Name": "TaskTime" }
  ],
  "summarizers": [ "PauseCount", "PauseDuration", "TaskTime" ]
}